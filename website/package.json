{
  "name": "@ssml-editor/website",
  "version": "1.0.4",
  "private": true,
  "description": "SSML editor demo website",
  "homepage": "https://www.ssmleditor.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssml-editor/ssml-editor.git"
  },
  "bugs": {
    "url": "https://github.com/ssml-editor/ssml-editor/issues"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start ./ --no-open",
    "start:cn": "docusaurus start ./ --locale zh-Hans --no-open",
    "build": "docusaurus build ./",
    "swizzle": "docusaurus swizzle",
    "deploy": "gh-pages -d ./build",
    "clear": "docusaurus clear ./",
    "serve": "docusaurus serve ./ --build true --no-open",
    "write-translations": "docusaurus write-translations ./ --locale zh-Hans",
    "write-heading-ids": "docusaurus write-heading-ids ./",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/plugin-ideal-image": "^3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/remark-plugin-npm2yarn": "3.9.2",
    "@mdx-js/react": "^3.1.1",
    "antd": "^5.29.3",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-zmage": "^1.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@types/react": "^19.2.14",
    "gh-pages": "^6.3.0",
    "typescript": "catalog:"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=10.24.0"
  }
}
