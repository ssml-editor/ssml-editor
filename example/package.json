{
  "name": "@ssml-editor/example",
  "private": true,
  "version": "1.0.3",
  "description": "all example are settled here",
  "homepage": "https://www.ssmleditor.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssml-editor/ssml-editor.git"
  },
  "bugs": {
    "url": "https://github.com/ssml-editor/ssml-editor/issues"
  },
  "license": "MIT",
  "author": "hz <beijingren@vip.qq.com>",
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=10.24.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "style": "./dist/styles/style.css",
      "import": "./dist/index.es.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs.js"
    },
    "./styles/*": "./dist/styles/*"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "pnpm dev",
    "dev": "vite --force --cors --mode development",
    "build": "pnpm rm:dist && pnpm type-check && pnpm build-only",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --noEmit --composite false",
    "rm:dist": "rimraf dist"
  },
  "dependencies": {
    "@ssml-editor/editor-vue": "workspace:^",
    "axios": "^1.13.2",
    "axios-mock-adapter": "^2.1.0",
    "element-plus": "catalog:",
    "slate-vue3": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@tailwindcss/vite": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "postcss": "catalog:",
    "postcss-nested": "catalog:",
    "rimraf": "catalog:",
    "tailwind-scrollbar": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "unplugin-element-plus": "catalog:",
    "vite": "catalog:",
    "vite-plugin-html": "catalog:",
    "vue-tsc": "catalog:"
  }
}
